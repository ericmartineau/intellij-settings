      <option name="JAVA_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="cnn" value="com.google.common.base.Preconditions.checkNotNull($VAR$, &quot;$VAR$ must not be null&quot;);" toReformat="true" toShortenFQNames="true">
