      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="imb" value="Map&lt;$type$, $type2$&gt; $var$ = new com.google.common.collect.ImmutableMap.Builder&lt;$type$, $type2$&gt;().build()" toReformat="true" toShortenFQNames="true">
