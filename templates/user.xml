<templateSet group="user">
  <template name="anb" value="com.google.common.base.Preconditions.checkArgument(!org.apache.commons.lang3.StringUtils.isBlank($VAR$), &quot;$VAR$ not blank&quot;);" toReformat="true" toShortenFQNames="true">
